# GPT-OSS Provider Configuration
# Copy this file to .env and fill in your actual values

# Required: Authentication credentials
SESSION_ID=your_session_id_here
FINGERPRINT=your_fingerprint_here

# Optional: API configuration
API_BASE_URL=https://chat-gpt-oss.com

# Optional: Model behavior configuration
# reasoning_effort options: low, medium, high
GPTOSS_REASONING_EFFORT=high

# verbosity options: low, medium, high (only applies to gpt-5-nano model)
GPTOSS_VERBOSITY=high
